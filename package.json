{
  "name": "icons-library",
  "version": "1.0.0",
  "displayName": "Icons Library",
  "description": "Easily browse and copy icons from popular free icon packs",
  "author": "Cristian Militaru",
  "publisher": "crsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/crsmilitaru97/icons-library"
  },
  "license": "ISC",
  "keywords": [
    "icons",
    "library",
    "codicons",
    "fontawesome",
    "fabrands",
    "primeicons",
    "bootstrap",
    "material",
    "heroicons",
    "lucide",
    "feather",
    "tabler",
    "remixicon",
    "lineicons",
    "simpleicons",
    "eva",
    "boxicons",
    "iconoir",
    "phosphor"
  ],
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onView:icons-library.icons-view"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "icons-library-container",
          "title": "Icons Library",
          "icon": "assets/icon.png"
        }
      ]
    },
    "views": {
      "icons-library-container": [
        {
          "type": "webview",
          "id": "icons-library.icons-view",
          "name": "Icons Library",
          "icon": "assets/icon.png"
        }
      ]
    },
    "configuration": {
      "title": "Icons Library",
      "properties": {
        "iconsLibrary.enabledPacks": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "codicons",
              "fontawesome",
              "fabrands",
              "primeicons",
              "bootstrap",
              "material",
              "heroicons",
              "lucide",
              "feather",
              "tabler",
              "remixicon",
              "lineicons",
              "simpleicons",
              "eva",
              "boxicons",
              "iconoir",
              "phosphor"
            ]
          },
          "default": [
            "codicons",
            "fontawesome",
            "fabrands",
            "primeicons",
            "bootstrap",
            "material",
            "heroicons",
            "lucide",
            "feather",
            "tabler",
            "remixicon",
            "lineicons",
            "simpleicons",
            "eva",
            "boxicons",
            "iconoir",
            "phosphor"
          ],
          "description": "Icon packs to display in the library"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "verify": "npx ts-node tools/verify-packs.ts",
    "versions": "npx ts-node tools/fetch-versions.ts",
    "compile": "tsc -p ./",
    "package": "npx vsce package",
    "publish:vsix": "npx vsce publish",
    "publish:ovsx": "npx ovsx publish",
    "publish": "npm run publish:vsix && npm run publish:ovsx"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.104.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  }
}